<div id="kagenomise-cart-cartviewlet">
    <div style="float:left;font-size:12px">
        This ordering system is still under testing and development, if you have trouble
        ordering through here, email me at izhar@kagesenshi.org
    </div>
    <div class="cartCheckout cartButton">
        <a href="javascript:;" class="simpleCart_checkout">Checkout</a>
    </div>
    <div class="cartInfo cartButton"><span class="simpleCart_quantity"></span> items</div>
    <div class="visualClear"></div>
    <div id="cartPopover">
        <div id="triangle">&#x25B2;</div>
        <div class="simpleCart_items"></div>
        <div id="cartData" class="clearfix">
            <div class="left"><strong>Items: </strong><span class="simpleCart_quantity"></span></div>
            <div class="right"><strong>Total: </strong><span class="simpleCart_total"></span></div>
        </div>
        <div id="popoverButtons" class="clearfix">
            <a href="javascript:;" class="simpleCart_checkout hudbtn primary right">Checkout</a>
        </div>
    </div>
</div>
